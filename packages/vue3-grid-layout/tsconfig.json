{
	"extends": "@vue/tsconfig/tsconfig.web.json",
	"include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/**/*.json", "./package.json"],
	"exclude": ["src/**/__tests__/*"],
	"compilerOptions": {
		"composite": true,
		"resolveJsonModule": true,
		"rootDir": ".",
		"baseUrl": "./",
		"paths": {
			"@*": ["src/*"]
		}
	}
}
